{
  "cleanUrls": true,
  "routes": [
    {
      "src": "/frame",
      "dest": "/frame.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8",
        "Cache-Control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "X-Frame-Options": "ALLOWALL"
      }
    }
  ]
}
